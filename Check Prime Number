#include<stdio.h>
int main() {
    int n;
    printf("Enter n: ");
    scanf("%d",&n);

    int a=0;
    for(int i=2; i<n; i++){
        if(n%i==0) {
            a=1;
            break;
        }
    }
    if(n==1 || n==0){
        printf("not prime");
    }
    else if(a==0) {
        printf("prime");
    }
    else{
        printf("not prime");
    }
}
