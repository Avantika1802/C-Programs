#include<stdio.h>
int main() {
    int n;int digit;
    printf("Enter n: ");
    scanf("%d",&n);
    printf("Enter digit: ");
    scanf("%d",&digit);
    int x=0;
    int count=0;
    
   while(n>0){
       x=n%10;
       if(x==digit){
           count++;
       }
       n=n/10;
   }
    printf("%d",count);
}
